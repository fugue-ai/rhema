{
  "name": "vscode-extension",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "editor-plugins/vscode/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm build",
        "cwd": "editor-plugins/vscode"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm test",
        "cwd": "editor-plugins/vscode"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm lint",
        "cwd": "editor-plugins/vscode"
      }
    },
    "package": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm package",
        "cwd": "editor-plugins/vscode"
      }
    }
  },
  "tags": ["type:lib", "scope:editor", "platform:vscode"]
} 