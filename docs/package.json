{
  "name": "docs",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "exports": {
    ".": {
      "svelte": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && node scripts/build-docs.js",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    "clean": "rm -rf .svelte-kit build",
    "clean:all": "rm -rf .svelte-kit build node_modules"
  },
  "devDependencies": {
    "@iconify-json/ri": "^1.2.5",
    "@mdx-js/mdx": "^3.1.0",
    "@mdx-js/rollup": "^3.1.0",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.22.0",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "@svelteness/kit-docs": "^1.1.5",
    "@vitest/ui": "^3.2.4",
    "clsx": "^2.1.1",
    "shiki": "^3.9.2",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "unplugin-icons": "^22.2.0",
    "vite": "^7.0.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@types/prismjs": "^1.26.5",
    "kitdocs": "^0.0.35",
    "marked": "^16.1.1",
    "mathjax": "^3.2.2",
    "prismjs": "^1.30.0"
  }
}
