{
  "name": "docs",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "docs/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm build",
        "cwd": "docs"
      }
    },
    "dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm dev",
        "cwd": "docs"
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm preview",
        "cwd": "docs"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm test",
        "cwd": "docs"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm lint:biome",
        "cwd": "docs"
      }
    },
    "format": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm format",
        "cwd": "docs"
      }
    },
    "check": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm check",
        "cwd": "docs"
      }
    }
  },
  "tags": ["type:app", "scope:docs"]
} 